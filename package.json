{
    "name": "stata-language",
    "displayName": "Stata language",
    "description": "Provides syntax highlighting for the Stata statistical software package (.do and .ado files)",
    "version": "1.0.0",
    "publisher": "mdob2k",
    "engines": {
        "vscode": "^1.0.0"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mdob2k/vscode-stata-language/issues"
    },
    "homepage": "https://github.com/mdob2k/vscode-stata-language",
    "repository": {
        "type": "git",
        "url": "https://github.com/mdob2k/vscode-stata-language.git"
    },
    "categories": [
        "Languages"
    ],
    "galleryBanner": {
        "color": "#145e8f",
        "theme": "dark"
    },
    "contributes": {
        "languages": [
            {
                "id": "stata",
                "aliases": [
                    "Stata",
                    "stata"
                ],
                "extensions": [
                    ".do",
                    ".ado"
                ],
                "configuration": "./stata.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "stata",
                "scopeName": "source.stata",
                "path": "./syntaxes/stata.tmLanguage"
            }
        ]
    }
}